{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "p_url": {
      "type": "text"
    },
    "p_image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "p_likes": {
      "type": "biginteger"
    },
    "p_author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "p_comment": {
      "model": "comment",
      "via": "c_post"
    },
    "p_caption": {
      "type": "richtext"
    }
  }
}
